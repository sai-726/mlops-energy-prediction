# AWS Credentials
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here
AWS_DEFAULT_REGION=us-east-1

# S3 Configuration
S3_BUCKET_NAME=mlops-energy-artifacts

# Neon PostgreSQL Configuration
POSTGRES_USER=your_neon_user
POSTGRES_PASSWORD=your_neon_password
POSTGRES_HOST=your_neon_host.neon.tech
POSTGRES_PORT=5432
POSTGRES_DB=mlflow_db

# MLflow Configuration
MLFLOW_TRACKING_URI=http://your-ec2-public-ip:5000
MLFLOW_EXPERIMENT_NAME=energy-consumption-prediction

# EC2 Configuration (for reference)
EC2_PUBLIC_IP=your_ec2_public_ip
EC2_KEY_PATH=path/to/your/key.pem
